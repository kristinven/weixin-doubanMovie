/**app.wxss**/
page{
  width: 100%;
  height: 100%;
  background-color: #f9f9f9;
  font-size: 32rpx;
}
.container {
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  position: relative;
} 
.search-movie{
  position: fixed;
  top: 0;
  left: 0;
  display: flex;
  flex-direction: row;
  align-items: center;
  width: 100%;
  height: 86rpx;
  background-color: #fff;
  /* margin-bottom: 50rpx; */
  padding: 20rpx 30rpx;
  box-sizing: border-box;
  border-width: 1px 0;
  border-style: solid;
  border-color: #eee;
}
 .search-movie .search-icon{
   margin-right: 30rpx;
 }  
 .search-movie .search-placeholder{
   font-size: 28rpx;
   color: #9e9e9e;
 }

